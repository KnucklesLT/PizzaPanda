  <% if (!user) { %>
    <script defer src="https://accounts.google.com/gsi/client"></script>
  <% } %>
</head>
<body>
  <nav>
    <img id="logo" src="/img/PizzaPandaLogo.png" alt="PizzaPandaLogo">
    <a href="/meals" class="right">All Meals</a>
    <% if (user) { %>
      <a href="/meals/new">Add Meal</a>
      <a href="/days">View Planner</a>
      <a href="/auth/logout" class="right">
        <button type="submit">Logout</button>
      </a>
      <% } else { %>
      <div 
        class="right"
        id="g_id_onload"
        data-client_id="<%= googleClientID %>"
        data-login_uri="/auth/google"
        data-auto_prompt="false"
      ></div>
      <div 
        class="g_id_signin right"
        data-type="standard"
        data-size="large"
        data-theme="outline"
        data-text="sign_in_with"
        data-shape="rectangular"
        data-logo_alignment="left"
      ></div>
    <% } %>
  </nav>
